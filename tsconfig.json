{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": false,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noLib": false,
    "sourceMap": true,
    "pretty": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noFallthroughCasesInSwitch": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "./dist/out-tsc",
    "baseUrl": "src",
    "moduleResolution": "node",
    "typeRoots": [
      "node_modules/@types",
      "node_modules"
    ],
    "types": [
      "node",
      "jasmine",
      "systemjs"
    ],
    "lib": [
      "es2016",
      "dom"
    ]
  },
  "compileOnSave": false,
  "exclude": [
    "app/main-prod.ts"
  ]
}
